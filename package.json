{
  "name": "entity-docs",
  "version": "0.0.13",
  "type": "module",
  "homepage": "",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./decorators": {
      "import": "./esm/decorators/index.js",
      "require": "./cjs/decorators/index.js",
      "types": "./types/decorators/index.d.ts"
    },
    "./generator": {
      "import": "./esm/generator/EntityGenerator.js",
      "require": "./cjs/generator/EntityGenerator.js",
      "types": "./types/generator/EntityGenerator.d.ts"
    },
    "./interfaces": {
      "import": "./esm/interfaces/index.js",
      "require": "./cjs/interfaces/index.js",
      "types": "./types/interfaces/index.d.ts"
    }
  },
  "files": [
    "cjs",
    "esm",
    "types",
    "README.md"
  ],
  "repository": {
    "url": "https://github.com/AlvXodus/SchemaDocs"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:clean": "rimraf build",
    "build:esm": "tsc --project tsconfig.prod.esm.json",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "copy-files": "cp package.json ./build/ && cp README.md ./build/",
    "release": "npm run build && npm run copy-files && npm publish ./build"
  },
  "keywords": [
    "entity",
    "documentation",
    "schema",
    "tables",
    "html"
  ],
  "author": "AlvXodus",
  "license": "MIT",
  "description": "Generate schema docs for various schema types",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.13.7",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "rollup-plugin-dts": "^6.1.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
